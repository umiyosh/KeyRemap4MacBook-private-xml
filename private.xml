        <identifier>private.umiyosh_Returnalias</identifier>
        <autogen>--KeyToKey-- KeyCode::SEMICOLON, VK_SHIFT, KeyCode::SEMICOLON, VK_SHIFT</autogen>
        <autogen>--KeyToKey-- KeyCode::SEMICOLON, ModifierFlag::CONTROL_L   , KeyCode::SEMICOLON </autogen>
        <autogen>--KeyToKey-- KeyCode::SEMICOLON, KeyCode::RETURN </autogen>
    </item>
    <list>
        <appdef>
            <appname>Excel</appname>
            <equal>com.microsoft.Excel</equal>
        </appdef>
        <item>
          <name>Excel home/end custom</name>
          <identifier>private.umiyosh_Excel_custom</identifier>
          <only>Excel</only>
          <autogen>--KeyToKey-- KeyCode::COMMA, ModifierFlag::OPTION_L, KeyCode::HOME </autogen>
          <autogen>--KeyToKey-- KeyCode::DOT  , ModifierFlag::OPTION_L, KeyCode::END </autogen>
          <autogen>--KeyToKey-- KeyCode::F2   , KeyCode::U            , ModifierFlag::CONTROL_L </autogen>
        </item>
    </list>
    <item>
        <name> umiyosh:alt custom HOME/END </name>
        <identifier>private.umiyosh_HOME_END</identifier>
        <autogen>--KeyToKey-- KeyCode::COMMA, ModifierFlag::CONTROL_L|ModifierFlag::OPTION_L, KeyCode::HOME </autogen>
        <autogen>--KeyToKey-- KeyCode::DOT  , ModifierFlag::CONTROL_L|ModifierFlag::OPTION_L, KeyCode::END </autogen>
        <autogen>--KeyToKey-- KeyCode::COMMA, ModifierFlag::OPTION_L                        , KeyCode::A              , ModifierFlag::CONTROL_L </autogen>
        <autogen>--KeyToKey-- KeyCode::DOT  , ModifierFlag::OPTION_L                        , KeyCode::E              , ModifierFlag::CONTROL_L </autogen>
    </item>
    <item>
        <name> umiyosh:Finder custom</name>
        <identifier>private.umiyosh_Finder_tabchange</identifier>
        <only>Finder</only>
        <autogen>--KeyToKey-- KeyCode::JIS_BRACKET_RIGHT, ModifierFlag::COMMAND_L|ModifierFlag::SHIFT_L, KeyCode::TAB, ModifierFlag::CONTROL_L </autogen>
        <autogen>--KeyToKey-- KeyCode::JIS_BRACKET_LEFT , ModifierFlag::COMMAND_L|ModifierFlag::SHIFT_L, KeyCode::TAB, ModifierFlag::CONTROL_L|ModifierFlag::SHIFT_L </autogen>
    </item>
    <list>
        <item>
          <name>umiyosh:Crhome custom</name>
          <identifier>private.umiyosh:Crhome_custom</identifier>
          <only>Chrome</only>
          <autogen>--KeyToKey-- KeyCode::COMMA, KeyCode::JIS_EISUU , KeyCode::COMMA  </autogen>
        </item>
    </list>
    <list>
        <item>
          <name>LeaveInsMode with EISUU(Terminal)</name>
          <identifier>private.app_terminal_esc_with_eisuu</identifier>
          <autogen>--KeyToKey-- KeyCode::JIS_BRACKET_LEFT , ModifierFlag::CONTROL_L, KeyCode::ESCAPE             , KeyCode::JIS_EISUU  </autogen>
          <autogen>--KeyToKey-- KeyCode::ESCAPE           , KeyCode::ESCAPE        , KeyCode::JIS_EISUU</autogen>
          <autogen>--KeyToKey-- KeyCode::C                , VK_CONTROL             , KeyCode::C                  , VK_CONTROL                    , KeyCode::JIS_EISUU</autogen>
        </item>
    </list>
    <list>
        <item>
          <name>restain minimize window</name>
          <identifier>private.restain_minimize_window</identifier>
